{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "interface-name": false,
        "no-console": false
    },
    "rulesDirectory": []
}